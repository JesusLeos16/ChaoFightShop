{
  "rewrites": [
    {
      "source": "/((?!assets|manifest.json|favicon.ico|.*\\.(png|jpg|jpeg|svg|gif|webp|ico)).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, must-revalidate"
        }
      ]
    }
  ]
}

